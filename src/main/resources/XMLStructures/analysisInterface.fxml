<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.dii.client.controllers.ControllerAnalysisInterface">
   <children>
      <HBox layoutX="20.0" layoutY="66.0" prefHeight="341.0" prefWidth="767.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="463.0" prefWidth="329.0">
               <children>
                  <BarChart fx:id="bar_chart_mptags">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </BarChart>
                  <Button fx:id="button_update" contentDisplay="CENTER" mnemonicParsing="false" onAction="#eventButtonUpdate" text="Update" />
               </children>
            </VBox>
            <VBox prefHeight="268.0" prefWidth="201.0">
               <children>
                  <ListView fx:id="list_view_mptags_location" prefHeight="319.0" prefWidth="181.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="text_field_location" text="location" />
                        <Button fx:id="button_search" mnemonicParsing="false" onAction="#eventButtonSearch" prefHeight="39.0" prefWidth="69.0" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="268.0" prefWidth="200.0">
               <children>
                  <Label text="User Ranking" />
                  <ListView fx:id="list_view_mpusers" onMouseClicked="#eventSelectItem" prefHeight="254.0" prefWidth="180.0" />
               </children>
            </VBox>
         </children></HBox>
      <Button fx:id="button_back" contentDisplay="TOP" layoutX="390.0" layoutY="14.0" mnemonicParsing="false" onAction="#eventButtonBack" prefHeight="25.0" prefWidth="70.0" text="Back" />
      <Label fx:id="label_username" layoutX="32.0" layoutY="18.0" prefHeight="17.0" prefWidth="88.0" text="Username" />
   </children>
</AnchorPane>
