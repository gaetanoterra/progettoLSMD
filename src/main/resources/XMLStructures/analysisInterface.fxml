<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.dii.client.controllers.ControllerAnalysisInterface">
   <children>
      <VBox prefHeight="700.0" prefWidth="800.0">
         <children>
            <HBox prefHeight="36.0" prefWidth="800.0">
               <children>
                  <Label fx:id="label_username" prefHeight="17.0" prefWidth="88.0" text="Username" />
                  <Button fx:id="button_back" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#eventButtonBack" prefHeight="25.0" prefWidth="70.0" text="Back" />
               </children>
            </HBox>
            <HBox prefHeight="340.0" prefWidth="790.0">
               <children>
                  <VBox prefHeight="416.0" prefWidth="254.0">
                     <children>
                        <Label text="Most Popular Tags per Location">
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="list_view_mptags_location" prefHeight="348.0" prefWidth="234.0">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </ListView>
                        <HBox prefHeight="33.0" prefWidth="254.0">
                           <children>
                              <TextField fx:id="text_field_location" prefHeight="25.0" prefWidth="123.0" text="location">
                                 <HBox.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </HBox.margin>
                              </TextField>
                              <Button fx:id="button_search" mnemonicParsing="false" onAction="#eventButtonSearch" text="Search" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="416.0" prefWidth="250.0">
                     <children>
                        <Label text="Experts by Tag">
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="list_view_experts" onMouseClicked="#eventSelectExpert" prefHeight="416.0" prefWidth="206.0">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </ListView>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <TextField fx:id="text_field_experts" prefHeight="25.0" prefWidth="123.0">
                                 <HBox.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </HBox.margin>
                              </TextField>
                              <Button fx:id="button_search_experts" mnemonicParsing="false" onAction="#eventSearchExperts" text="Search" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="341.0" prefWidth="253.0">
                     <children>
                        <Label text="User Ranking">
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="list_view_mpusers" onMouseClicked="#eventSelectItem" prefHeight="348.0" prefWidth="273.0">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </ListView>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="326.0" prefWidth="800.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="233.0" prefWidth="522.0">
                     <children>
                        <BarChart fx:id="bar_chart_mptags" prefHeight="261.0" prefWidth="502.0">
                          <xAxis>
                            <CategoryAxis side="BOTTOM" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis side="LEFT" />
                          </yAxis>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </BarChart>
                        <Button fx:id="button_update" contentDisplay="CENTER" mnemonicParsing="false" onAction="#eventButtonUpdate" text="Update">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox prefHeight="313.0" prefWidth="235.0">
                     <children>
                        <Label text="Hot Topics">
                           <padding>
                              <Insets bottom="10.0" right="10.0" />
                           </padding>
                        </Label>
                        <TableView fx:id="table_view_hot_topics" onMouseClicked="#eventShowHotUser" prefHeight="291.0" prefWidth="235.0">
                          <columns>
                            <TableColumn fx:id="table_column_user" prefWidth="75.0" text="User" />
                            <TableColumn fx:id="table_column_tags" prefWidth="159.0" text="Tags" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
